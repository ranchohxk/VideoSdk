<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/opreate_bar_bg_color"
    android:minWidth="200dp">
    <android.support.v7.widget.LinearLayoutCompat
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingLeft="@dimen/size_10_dp"
        android:paddingRight="@dimen/size_10_dp"
        app:divider="@drawable/diliver"
        app:showDividers="middle"
        app:dividerPadding="2dp">

    <TextView
        android:id="@+id/params_face_vector_txt"
        style="@style/face_params_txt_style"
        android:text="@string/params_vector_str"/>

    </android.support.v7.widget.LinearLayoutCompat>

</LinearLayout>
